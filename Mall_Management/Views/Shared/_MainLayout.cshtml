    <!--begin::Javascript-->
    <script>
        var hostUrl = "assets/";
    </script>
    <!--begin::Global Javascript Bundle(used by all pages)-->
    <script src="~/Scripts/AdminJS/plugins.bundle.js"></script>
    <script src="~/Scripts/AdminJS/scripts.bundle.js"></script>
    <script src="~/Scripts/custom-notifi.js"></script>
    @RenderSection("scripts", required: false)
</body>
<!--end::Body-->
</html>
@section script{
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- jQuery (cần thiết cho các plugin hoặc hàm AJAX) -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <!-- Các file JavaScript của bạn -->
    <script src="/path/to/brands.js"></script>

}